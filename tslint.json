{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "indent": [true, "tabs", 2],
        "quotemark": false,
        "member-access": false,
        "max-classes-per-file": false,
        "curly": false,
        "object-literal-sort-keys": false,
        "no-console": false
    }
}