{
  "compilerOptions": {
    "lib": ["dom", "es6", "es2016.array.include", "es2017.object"],
    "target": "es5",
    "moduleResolution": "node",
    "incremental": true,
    "downlevelIteration": true,
    "jsx": "react",
    "outDir": "public/js",
    "strict": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "./node_modules/*"
  ]
}
